{
  "name": "herolo",
  "homepage": "https://YogiHa.github.io/For-Forecast",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.6.0",
    "firebase": "7.3.0",
    "gh-pages": "2.1.1",
    "husky": "3.0.9",
    "lint-staged": "9.4.3",
    "moment": "2.24.0",
    "prettier": "1.19.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-moment": "0.9.6",
    "react-particles-js": "2.7.0",
    "react-redux": "7.1.3",
    "react-redux-firebase": "2.4.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.0.1",
    "react-toggle-button": "2.2.0",
    "redux": "4.0.4",
    "redux-firestore": "0.9.0",
    "redux-thunk": "2.3.0",
    "use-position": "0.0.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "server/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "selenium/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node_modules/.bin/mocha selenium/test.js --timeout 10000",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chromedriver": "119.0.1",
    "mocha": "6.2.2",
    "selenium-webdriver": "4.0.0-alpha.5"
  }
}
